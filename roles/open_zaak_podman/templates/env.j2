DB_HOST={{ openzaak_db_host }}
DB_PORT={{ openzaak_db_port | string }}
DB_NAME={{ openzaak_db_name }}
DB_USER={{ openzaak_db_username }}
DB_PASSWORD={{ openzaak_db_password }}

SECRET_KEY={{ openzaak_secret_key }}
ALLOWED_HOSTS=localhost,{{ openzaak_domain }}

CELERY_BROKER_URL={{ openzaak_celery_broker_url }}
CELERY_RESULT_BACKEND={{ openzaak_celery_result_backend }}

CACHE_DEFAULT=localhost:6379/{{ openzaak_cache_db }}
CACHE_AXES=localhost:6379/{{ openzaak_cache_db }}

IS_HTTPS={{ 'yes' if openzaak_ssl else 'no' }}

{% if openzaak_sentry_dsn %}SENTRY_DSN={{ openzaak_sentry_dsn }}{% endif %}

{% for envvar in openzaak_extra_env %}
{{ envvar.name }}={{ envvar.value }}
{% endfor %}
