DB_HOST={{ opennotificaties_db_host }}
DB_PORT={{ opennotificaties_db_port | string }}
DB_NAME={{ opennotificaties_db_name }}
DB_USER={{ opennotificaties_db_username }}
DB_PASSWORD={{ opennotificaties_db_password }}

SECRET_KEY={{ opennotificaties_secret_key }}
ALLOWED_HOSTS=localhost,{{ opennotificaties_domain }}

PUBLISH_BROKER_URL={{ opennotificaties_publish_broker_url }}
CELERY_BROKER_URL={{ opennotificaties_celery_broker_url }}
CELERY_RESULT_BACKEND={{ opennotificaties_celery_result_backend }}

CACHE_DEFAULT=localhost:6379/{{ opennotificaties_cache_db }}
CACHE_AXES=localhost:6379/{{ opennotificaties_cache_db }}

IS_HTTPS={{ 'yes' if opennotificaties_ssl else 'no' }}

{% if opennotificaties_sentry_dsn %}SENTRY_DSN={{ opennotificaties_sentry_dsn }}{% endif %}

{% for envvar in opennotificaties_extra_env %}
{{ envvar.name }}={{ envvar.value }}
{% endfor %}
